{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "npm",
			"script": "compile",
			"group": "build",
			"isBackground": true,
			"presentation": {
				"reveal": "never"
			},
			"problemMatcher": "$tsc-watch"
		},
		{
			"type": "npm",
			"script": "lint",
			"problemMatcher": "$tslint5"
		},
		{
			"type": "npm",
			"script": "install",
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}
